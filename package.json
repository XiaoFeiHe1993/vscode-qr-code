{
  "name": "vscode-qr-code",
  "displayName": "qr-code",
  "description": "generate QR-code by you select text",
  "version": "1.0.0",
  "publisher": "xiaofeihe",
  "engines": {
    "vscode": "^1.3.0"
  },
  "scripts": {
    "vscode:prepublish": "tsc -p ./",
    "compile": "tsc -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install"
  },
  "categories": [
    "Debuggers"
  ],
  "icon": "images/logo.png",
  "bugs": {
    "url": "https://github.com/XiaoFeiHe1993/vscode-qr-code/issues",
    "email": "1712776213@qq.com"
  },
  "homepage": "https://github.com/XiaoFeiHe1993/vscode-qr-code/blob/master/README.md",
  "repository": {
    "type": "git",
    "url": "https://github.com/XiaoFeiHe1993/vscode-qr-code.git"
  },
  "activationEvents": [
    "onCommand:extension.QR-code"
  ],
  "license": "MIT",
  "main": "./out/src",
  "contributes": {
    "menus": {
      "editor/context": [
        {
          "command": "extension.QR-code",
          "group": "npm"
        }
      ]
    },
    "commands": [
      {
        "command": "extension.QR-code",
        "title": "generate QR-code"
      }
    ]
  },
  "devDependencies": {
    "@types/node": "^13.1.4",
    "typescript": "^3.7.4",
    "vscode": "^1.1.36"
  },
  "dependencies": {
    "qrcode": "^1.4.4"
  }
}
